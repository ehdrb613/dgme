import java.util.Scanner;

public class Ddi {

	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);
		System.out.print("년도 입력 : ");
		int input = s.nextInt();
		
		int ddi = input%12;
		System.out.println(ddi);
		if(ddi == 1)
			System.out.println("닭");
		else if(ddi ==2)
			System.out.println("개");
	}

}
